{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pieceoffurniture</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pieceOfFurniture.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ pieceOfFurniture.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('pieceoffurniture_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('pieceoffurniture_edit', { 'id': pieceOfFurniture.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
